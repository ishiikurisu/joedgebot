{
    "available": {
        "py": "python",
        "rb": "ruby",
        "java": "java",
        "txt": "text",
        "c": "c",
        "cpp": "c++",
        "go": "go",
        "lua": "lua"
    },
    "about": {
        "python": {
            "kind": "interpreted",
            "run": "python %s"
        },
        "ruby": {
            "kind": "interpreted",
            "run": "ruby %s"
        },
        "text": {
            "kind": "interpreted",
            "run": "cat %s"
        },
        "c": {
            "kind": "compiled",
            "build": "gcc %s -o %s.out",
            "run": "./%s.out",
            "clean": "rm %s.out"
        }
    }
}
